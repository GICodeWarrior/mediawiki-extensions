                    SPACE[ ]
                END CELL
                BEGIN CELL
                    SPACE[ ]
                    WORD[a]
                    NEWLINE
                END CELL
                BEGIN CELL
                    SPACE[ ]
                    WORD[key]
                    SPECIAL[=]
                    WORD[value]
                    SPACE[ ]
                END CELL
                BEGIN CELL
                    SPACE[ ]
                    WORD[a]
                    NEWLINE
                END CELL
            END ROW
            BEGIN ROW key=" valeu;;'"
