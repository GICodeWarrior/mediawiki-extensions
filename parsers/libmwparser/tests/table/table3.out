                    WORD[value]
                    SPACE[ ]
                    WORD[key]
                    WORD[2]
                    SPECIAL[=]
                    SPECIAL["]
                    WORD[value]
                    SPECIAL["]
                    SPACE[ ]
                    WORD[key]
                    WORD[3]
                    SPECIAL[=]
                    SPECIAL[']
