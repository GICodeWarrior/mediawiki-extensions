# @(#) $Header$
#
# General build rules.

@include@ @q@@abs_top_srcdir@/mk/vars.mk@q@

.c.o:
	@echo "	$(_COMPILE) -c $<"
	@$(_COMPILE) -c $<

.cc.o:
	@echo "	$(_COMPILEXX) -c $<"
	@$(_COMPILEXX) -c $<

_dist: 
	[ -d $(DISTDEST)/$(_DISTPATH) ] || mkdir -p $(DISTDEST)/$(_DISTPATH)
	cp $(_MYDISTFILES) $(DISTDEST)/$(_DISTPATH)
	$(MAKE) _extradist

dist: _dist
