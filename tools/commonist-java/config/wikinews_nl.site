## configuration for a single wiki site

# identification
family	wikinews
language	nl

# network
protocol	http://
hostName	nl.wikinews.org
rawPath	/w/index.php
prettyPath	/wiki/
apiPath	/w/api.php

# config
charSet	UTF-8
titleCase	first-letter
uselang	nds

# nameSpaces
nameSpace	-2	'Media'
nameSpace	-1	'Speciaal'
nameSpace	0	''
nameSpace	1	'Overleg'
nameSpace	2	'Gebruiker'
nameSpace	3	'Overleg gebruiker'
nameSpace	4	'Wikinews'
nameSpace	5	'Overleg Wikinews'
nameSpace	6	'Bestand'
nameSpace	7	'Overleg bestand'
nameSpace	8	'MediaWiki'
nameSpace	9	'Overleg MediaWiki'
nameSpace	10	'Sjabloon'
nameSpace	11	'Overleg sjabloon'
nameSpace	12	'Help'
nameSpace	13	'Overleg help'
nameSpace	14	'Categorie'
nameSpace	15	'Overleg categorie'

# specialPages
specialPage	'Blockip'	'Blokkeren'
specialPage	'Filepath'	'Bestandspad'
specialPage	'Movepage'	'PaginaHernoemen'
specialPage	'Upload'	'Uploaden'
specialPage	'Userlogin'	'Aanmelden'
specialPage	'Userlogout'	'Afmelden'

