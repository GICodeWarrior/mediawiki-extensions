## configuration for a single wiki site

# identification
family	wikisource
language	pt

# network
protocol	http://
hostName	pt.wikisource.org
rawPath	/w/index.php
prettyPath	/wiki/
apiPath	/w/api.php

# config
charSet	UTF-8
titleCase	first-letter
uselang	nds

# nameSpaces
nameSpace	-2	'Media'
